# Environment Configuration Template
# Copy this file to .env and update the values

NODE_ENV=development

# Application Configuration
VITE_APP_NAME="Table API Integration"
VITE_APP_VERSION="0.0.0"
VITE_APP_DESCRIPTION="React Dashboard with API Integration"

# API Configuration
VITE_API_BASE_URL="https://dummyjson.com"
VITE_API_TIMEOUT=10000

# Authentication (uncomment and set when needed)
# VITE_AUTH_SECRET="your-auth-secret-here"
# VITE_JWT_SECRET="your-jwt-secret-here"

# Database Configuration (uncomment and set when using real backend)
# DATABASE_URL="your-database-url-here"
# DB_HOST="localhost"
# DB_PORT="5432"
# DB_NAME="your-database-name"
# DB_USER="your-username"
# DB_PASSWORD="your-password"

# Third-party Services (uncomment and set when needed)
# VITE_GOOGLE_ANALYTICS_ID="your-ga-id-here"
# VITE_SENTRY_DSN="your-sentry-dsn-here"

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_DATA=false

# Theme Configuration
VITE_DEFAULT_THEME="light"
VITE_THEME_STORAGE_KEY="vite-ui-theme"

# Cache Configuration
VITE_CACHE_DURATION=300000

# Development Configuration
VITE_DEV_TOOLS=true
VITE_MOCK_DELAY=1000

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME="your-cloud-name"
VITE_CLOUDINARY_API_KEY="your-api-key"
VITE_CLOUDINARY_API_SECRET="your-api-secret"
VITE_CLOUDINARY_UPLOAD_PRESET="your-upload-preset"